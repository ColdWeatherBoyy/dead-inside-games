<h1>Welcome to Minesweeper, {{username}}</h1>
<section class="grid">
  <script src="/js/game.js"></script>
  <div class="start-button">
    <button class="btn btn-primary" type="start">Start Game</button>
  </div>
  <div class="row">
    <a href="placehoder">Highscores</a>
    <button id="highscore-btn" class="modal">View Highscores</button>
    <span class="modal-content"></span>

    <a
      href="/api/userRoutes/logout/{{id}}"
      id="logout"
      class="btn-no-style"
    >Logout</a>

  </div>
  <script src="/js/logout.js"></script>
  <script src="/js/highscores.js"></script>
</section>